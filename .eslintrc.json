{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": 0,
        "comma-dangle": 0,
        "no-fallthrough": 0,
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "no-underscore-dangle": "error",
        "eol-last": ["error", "always"],
        "import/no-cycle": 1,
        "import/no-unresolved": 1,
        "max-classes-per-file": 0,
        "no-console": 0,
        "no-unused-vars": 0
    }
}
